{
  "manifest_version": 2,
  "name": "Jira Scrum Points",
  "version": "0.0.1",
  "description": "This is an extension that can be added to your browser to show the totaled scrum points on our engineering scrum boards.",
  "permissions": [ "tabs", "https://illuminatehealth.atlassian.net/secure/RapidBoard.jspa?*" ],
    "content_scripts": [ {
        "all_frames": true,
        "js": [ "lib/jquery-3.2.1.min.js", "content.js" ],
        "css": [ "lib/main.css" ],
        "matches": [ "https://illuminatehealth.atlassian.net/secure/RapidBoard.jspa?*" ]
    } ]
}
